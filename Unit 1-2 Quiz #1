#1
name = input()
Base = int(input())
Length = int(input())

#Name
print( 'Hello ' + name )
#Cube
print( Base * Base * Base / 7.5 )
#Circle
print( Base / 2 ** 2 * Length * 3.14 / 5.875 )
#Rectangle
print( Base * Base * Length / 7.5 )

#2
